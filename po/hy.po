#. warn about a short delay...
#: ../src/lxinput.c:528
msgid "Setting keyboard - please wait..."
msgstr "Ստեղնաշարի կարգավորում - խնդրում ենք սպասել..."

#: ../src/lxinput.c:677
msgid "Keyboard Layout..."
msgstr "Ստեղնաշարի դասավորություն..."

#: ../src/lxinput.c:772
msgid "LXInput autostart"
msgstr "LXInput ինքնուրույն մեկնարկ"

#: ../src/lxinput.c:773
msgid "Setup keyboard and mouse using settings done in LXInput"
msgstr "Կարգավորել ստեղնաշարը և մկնիկը օգտագործելով LXInput -ում կատարված կարգավորումները"

#: ../data/lxinput.ui.h:1
msgid "Mouse and Keyboard Settings"
msgstr "Մկնիկի և ստեղնաշարի կարգավորումներ"

#: ../data/lxinput.ui.h:2
msgid "Acceleration:"
msgstr "Արագություն՝"

#: ../data/lxinput.ui.h:3
msgid "Slow"
msgstr "Դանդաղ"

#: ../data/lxinput.ui.h:4
msgid "Fast"
msgstr "Արագ"

#: ../data/lxinput.ui.h:5
msgid "Motion"
msgstr "Շարժում"

#: ../data/lxinput.ui.h:6
msgid "Left handed (Swap left and right mouse buttons)"
msgstr "Ձախլիկ (փոխարկել մկնիկի ձախ և աջ կոճակները)"

#: ../data/lxinput.ui.h:7
msgid "Delay:"
msgstr "Դանդաղեցնել՝"

#: ../data/lxinput.ui.h:8
msgid "Double-click"
msgstr "Կրկնակի կտտացնել"

#: ../data/lxinput.ui.h:9
msgid "Mouse"
msgstr "Մկնիկ"

#: ../data/lxinput.ui.h:10
msgid "Delay before each key starts repeating"
msgstr "Դանդաղեցում մինչ յուրաքանչյուր ստեղն սկսվում է կրկնել"

#: ../data/lxinput.ui.h:11
msgid "Repeat delay:"
msgstr "Կրկնել ուշացումը"

#: ../data/lxinput.ui.h:12
msgid "Interval between each key repeat"
msgstr "Յուրաքանչյուր կրկնվող ստեղնի միջակայքը"

#: ../data/lxinput.ui.h:13
msgid "Long"
msgstr "Երկար"

#: ../data/lxinput.ui.h:14
msgid "Short"
msgstr "Կարճ"

#: ../data/lxinput.ui.h:15
msgid "Repeat interval:"
msgstr "Կրկնել միջակայքը՝"

#: ../data/lxinput.ui.h:16
msgid "Type in the following box to test your keyboard settings"
msgstr "Ստեղնաշարի կարգավորումները թեստավորելու համար մուտքագրել կից վանդակում"

#: ../data/lxinput.ui.h:17
msgid "Character Repeat"
msgstr "Նիշի կրկնում"

#: ../data/lxinput.ui.h:18
msgid "Beep when there is an error of keyboard input"
msgstr "Ձայն հանել, երբ ստեղնաշարի մուտքագրումը սխալ է"

#: ../data/lxinput.ui.h:19
msgid "Keyboard"
msgstr "Ստեղնաշար"

#: ../data/lxinput.ui.h:20
msgid "Model:"
msgstr "Տեսակէ"

#: ../data/lxinput.ui.h:21
msgid "Layout:"
msgstr "Դասավորություն՝"

#: ../data/lxinput.ui.h:22
msgid "Variant:"
msgstr "Տարբերակ՝"

#: ../data/lxinput.desktop.in.h:1
msgid "Keyboard and Mouse"
msgstr "Մկնիկի և ստեղնաշարի կարգավորումներ"

#: ../data/lxinput.desktop.in.h:2
msgid "Configure keyboard, mouse, and other input devices"
msgstr "Կարգաբերել մկնիկը, ստեղնաշարն ու այլ մուտքային սարքերը"

