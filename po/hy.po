#~ msgstr "Մուտքային սարի նախընտրանքներ"

#~ msgid "Sensitivity:"
#~ msgstr "Զգայունություն՝"

#~ msgid "High"
#~ msgstr "Բարձր"

#~ msgid "Low"
#~ msgstr "Ցածր"

#~ msgid "Touch Pad"
#~ msgstr "«Թաչփադ»"

#~ msgid "input-keyboard"
#~ msgstr "Ստեղնաշար"
